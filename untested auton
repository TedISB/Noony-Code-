void RiskyautonwinpointOFF(){
LaderalSpeed = 0.1; 

desiredTurnValue = 0.1;
Motor5.spin(forward,100,pct);
desiredValue = -4500;
wait(1000,msec);
desiredTurnValue = 315;
wait(1000,msec);
desiredValue = -6500;
wait(1000,msec);
desiredTurnValue = 135;
wait(1000,msec);
Motor5.spin(reverse,100,pct);
wait(250,msec);
desiredTurnValue = 270;
wait(1000,msec);
desiredValue = -8500;
wait(1000,msec);
desiredValue = -6500;
wait(1000,msec);
desiredTurnValue = 5;
wait(500,msec);
Motor5.spin(forward,100,pct);
desiredValue = -1500;
wait(1000,msec);
desiredTurnValue = 135;
wait(1000,msec);
desiredValue = -1000;
Motor5.spin(reverse,100,pct);
wait(1000,msec);
desiredTurnValue = 45;
Motor5.spin(forward,100,pct);
wait(1000,msec);
desiredValue = 0;
wait(500,msec);
desiredTurnValue = 180;
wait(750,msec);
Motor5.spin(reverse,100,pct);
desiredValue = 3500;
wait(750,msec);
desiredTurnValue = 135;
wait(750,msec);
desiredValue = 0;
wait(600,msec);
desiredTurnValue = 180;

}

