  desiredTurnValue = 6;
  wait(1000,msec);
  Motor5.spin(forward,100,pct);
  desiredValue = 6500;
  wait(1500,msec);
  desiredTurnValue = 25;
  wait(1000,msec);
  desiredValue = 500;
  wait(1000,msec);
  desiredTurnValue = 135;
  wait(1000,msec);
  desiredValue = -3500;
